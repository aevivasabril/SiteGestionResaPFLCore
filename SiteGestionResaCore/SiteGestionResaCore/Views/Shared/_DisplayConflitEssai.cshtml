@model SiteGestionResaCore.Models.ConsultConflitEssaiVM

<div class="modal fade" id="ConflitModal" tabindex="-1" role="dialog" aria-labelledby="myModal" aria-hidden="true">
    <div class="modal-dialog modal-xl navbar-inra">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Equipements de l'essai " @Model.TitreEssaiPrincipal " en conflit avec autre(s) essai(s)</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="modalcontent">
                <table class="table table-sm table-borderless">
                    <thead>
                        <tr>
                            <th scope="col">N° Projet</th>
                            <th scope="col">Titre Essai</th>
                            <th scope="col">Nom Equipement</th>
                            <th scope="col">Zone</th>
                            <th scope="col">Date Début</th>
                            <th scope="col">Date Fin</th>
                            <th scope="col">Responsable projet</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var y in Model.InfosConflits)
                        {
                            <tr>
                                <td>@y.NumProjet</td>
                                <td>@y.TitreEss</td>
                                <td>@y.NomEquipement</td>
                                <td>@y.ZoneEquipement</td>
                                <td>@y.DateDeb</td>
                                <td>@y.DateFin</td>
                                <td>@y.MailResponsablePrj</td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>
            <div class="modal-footer">
                <button type="button" id="submit" class="btn btn-secondary" data-dismiss="modal">Ok</button>
            </div>
        </div>
    </div>
</div>